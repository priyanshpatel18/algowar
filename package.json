{
  "name": "algowar",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db": "turbo run db:generate && turbo run db:migrate",
    "studio": "turbo run db:studio",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "turbo": "^2.0.11",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
